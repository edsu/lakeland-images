    "react-icons": "^2.2.7",
    "react-masonry-component": "^6.0.2",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.6.2"
  },
