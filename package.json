  "scripts": {
    "start": "webpack-dev-server --env dev",
    "build": "ENV=production webpack --progress --profile --colors",
    "clean": "rimraf dist/*",
    "zip": "mv dist lakeland && zip -r lakeland.zip lakeland && mv lakeland dist&& mv lakeland.zip static";
    "publish": "npm run build && npm run zip && git commit -m 'latest' dist ; git subtree push --prefix dist origin gh-pages"
  },
